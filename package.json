{
  "name": "php-snippets-pro",
  "displayName": "PHP Snippets Pro",
  "description": "âœ¨ Modern PHP snippets with a beautiful UI browser. Boost your productivity with customizable, searchable code snippets.",
  "version": "1.0.0",
  "publisher": "Shellomo",
  "icon": "media/icon.png",
  "license": "MIT",
  "repository": "https://github.com/Shellomo/vscode_ext_php-snippets-pro",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "PHP",
    "php",
    "snippets",
    "productivity",
    "code snippets"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "php-snippets.showSnippets",
        "title": "Show PHP Snippets (Quick Pick)"
      },
      {
        "command": "php-snippets.showSnippetsWebview",
        "title": "Show PHP Snippets (Browser)"
      }
    ],
    "snippets": [
      {
        "language": "php",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.x",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  }
}